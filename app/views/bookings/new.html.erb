<%= simple_form_for [@camera, @booking] do |f| %>
  <%= f.input :start_date,
               as: :string,
               input_html: {data: {controller: "datepicker"} } %>
  <%= f.input :finish_date,
               as: :string,
               input_html: {data: {controller: "datepicker"} } %>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
